
{% extends 'admin.html.twig' %}

{% block title %}Privatisation{% endblock %}

{% block body %}
    <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3">
        <h1>Privatisation</h1>
        <div class="btn-toolbar mb-2 mb-md-0">
            <a href="{{ path('admin_privatisation') }}">Retour</a>
            <a href="{{ path('admin_privatisation_new') }}" class="btn btn-sm btn-outline-primary">Ajouter un privatisation</a>
            <a href="{{ path('admin_privatisation_edit', {'id': privatisation.id}) }}" class="btn btn-sm btn-outline-primary">Ã‰diter</a>
            {{ include('admin/privatisation/_delete_form.html.twig') }}
        </div>
    </div>

    <table class="table">
        <tbody>
            <tr>
                <th>Capacite</th>
                <td>{{ privatisation.capacite }}</td>
            </tr>
            <tr>
                <th>Surface</th>
                <td>{{ privatisation.surface }}</td>
            </tr>
            <tr>
                <th>Service</th>
                <td>{{ privatisation.service }}</td>
            </tr>
            <tr>
                <th>Annonce</th>
                <td>{{ privatisation.annonce }}</td>
            </tr>
        </tbody>
    </table>
{% endblock %}



